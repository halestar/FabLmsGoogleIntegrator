{
    "name": "halestar/fablms-google-integrator",
    "description": "An integration module for FabLMS (dev.kalinec.net) to integrate with Google Services.",
    "type": "add-on",
    "license": "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "halestar\\FablmsGoogleIntegrator\\": "src/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "halestar\\FablmsGoogleIntegrator\\GoogleIntegratorServiceProvider"
        ]
      }
    },
    "authors": [
        {
            "name": "German Kalinec",
            "email": "german@kalinec.net"
        }
    ],
    "minimum-stability": "stable",
    "require":
    {
      "php": "^8.3",
      "laravel/framework": ">=12.0",
      "livewire/livewire": "^v3.5.4",
      "prism-php/prism": "^0.98.0",
      "prism-php/relay": "^1.3"
    }
}
